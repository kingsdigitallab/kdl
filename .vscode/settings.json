    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#6df1a2",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#eb792d",
    "statusBar.background": "#d16014",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#eb792d",
    "statusBarItem.remoteBackground": "#d16014",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#d16014",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#d1601499",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#d16014",
  "[nunjucks]": {
    "editor.defaultFormatter": "okitavera.vscode-nunjucks-formatter"
  }
