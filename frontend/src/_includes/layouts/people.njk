---
layout: layouts/base.njk
permalink: "about/people/{% if pagination.pageNumber > 0 %}{{ pagination.pageNumber + 1 }}/{% endif %}/"
---

<div class="wrapper">
    <ul start="{{ pagination.pageNumber * 10 + 1 }}">
        {%- for person in paginatedPeople -%}
            {% set kdlTeamMember = team | filter(person.agent.slug, 'slug') | first %}
            <li>
                <a {% if kdlTeamMember %}class="kdl-member"{% endif %}
                   href="{% route person.agent.slug, 'People' %}">{{ person.agent.name }}</a>
            </li>
        {%- endfor -%}
    </ul>
</div>

{% set label = "People" %}
{% include "partials/pagination.njk" %}
