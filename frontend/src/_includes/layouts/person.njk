---
layout: layouts/agent.njk
---

{% set agent = agentInstance.agent %}

{% if agent.memberOf %}
<div id="part-of">
  <h2>Part of</h2>
  <ul>
    {% for member in agent.memberOf|sort(false, false, "inProject.name") %}
    <li>
      <a href="/projects/{{ member.inProject.slug }}/">{{ member.inProject.name }}</a>
    </li>
    {% endfor %}
  </ul>
</div>
{% endif %}
