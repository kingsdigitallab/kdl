---
layout: layouts/base.njk
templateClass: tmpl-home
permalink: "projects/{% if pagination.pageNumber > 0 %}{{ pagination.pageNumber + 1 }}/{% endif %}/"
---

<header>
  {% set navKey = eleventyNavigation.key %} {% include "partials/breadcrumbs.njk" %}
  <h1>Projects</h1>
</header>

<ol start="{{ pagination.pageNumber * 10 + 1}}">
  {%- for project in paginatedProjects -%}
  <li>
    <a href="/projects/{{ project.slug }}/">
      <img src="https://images.pexels.com/photos/2117937/pexels-photo-2117937.jpeg?auto=compress&cs=tinysrgb&w=1200" alt="insert text">
      <h2>{{ project.name }}</h2>
      {% include "partials/project.njk" %}
    </a>
  </li>
  {%- endfor -%}
</ol>

{% set label = "Projects" %} {% include "partials/pagination.njk" %}
